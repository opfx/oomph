      <repository
          url="http://download.eclipse.org/mylyn/releases/latest"/>
    </setupTask>
    <setupTask
        xsi:type="setup.p2:P2Task"
        excludedTriggers="MANUAL"
        label="EGit"
        licenseConfirmationDisabled="true">
